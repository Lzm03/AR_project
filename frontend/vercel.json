{
  "rewrites": [
    {
      "source": "/models/(.*)",
      "destination": "/models/$1"
    },
    {
      "source": "/bg/(.*)",
      "destination": "/bg/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
